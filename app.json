{
  "expo": {
    "name": "UTHH",
    "slug": "UTHH",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icono.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ayer.UTHH",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "af99ed99-2801-4bb8-a41a-2298ada9015f"
      }
    }
  }
}
